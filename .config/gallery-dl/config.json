      "mode": "a",
      "format": "{asctime} {message}",
      "format-date": "%Y-%m-%dT%H:%M:%S"
    },

    "colors": {
      "success": "1;32",
      "skip": "2",
      "debug": "0;37",
      "info": "1;37",
      "warning": "1;33",
      "error": "1;31"
